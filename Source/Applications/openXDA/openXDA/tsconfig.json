{
  "compilerOptions": {
      "baseUrl": ".",
      "paths": { "*": ["types/*"] },
    "jsx": "react",
    "lib": [ "dom", "es5", "scripthost", "es2015" ],
    "noEmitOnError": true,
    "noImplicitAny": false,
    "removeComments": true,
    "sourceMap": true,
    "target": "es5",
    "typeRoots": [ "node_modules/@types" ]
  },
  "include": [ "wwwroot/Scripts/TSX", "wwwroot/Scripts/TS/*" ],
  "exclude": [
    "node_modules"
  ]
}
